<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletAnneeAcademique</servlet-name>
        <servlet-class>ServletAnneeAcademique</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletCompteUtilisateurs</servlet-name>
        <servlet-class>ServletCompteUtilisateurs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletCours</servlet-name>
        <servlet-class>ServletCours</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEtudiants</servlet-name>
        <servlet-class>ServletEtudiants</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletProfesseurs</servlet-name>
        <servlet-class>ServletProfesseurs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletPalmares</servlet-name>
        <servlet-class>ServletPalmares</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletNotes</servlet-name>
        <servlet-class>ServletNotes</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAnneeAcademique</servlet-name>
        <url-pattern>/ServletAnneeAcademique</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCompteUtilisateurs</servlet-name>
        <url-pattern>/ServletCompteUtilisateurs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCours</servlet-name>
        <url-pattern>/ServletCours</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEtudiants</servlet-name>
        <url-pattern>/ServletEtudiants</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletProfesseurs</servlet-name>
        <url-pattern>/ServletProfesseurs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletPalmares</servlet-name>
        <url-pattern>/ServletPalmares</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletNotes</servlet-name>
        <url-pattern>/ServletNotes</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
